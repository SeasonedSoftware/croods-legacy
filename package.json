    "docz": "^0.13.7",
    "docz-theme-default": "^0.13.7",
    "eslint": "^4.19.1",
    "eslint-config-universe": "^1.0.7",
    "gh-pages": "^1.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.1",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "rollup": "^0.54.0",
    "rollup-plugin-babel": "^4.0.1",
